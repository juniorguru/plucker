{
  "actorSpecification": 1,
  "name": "{{ cookiecutter.scraper_name }}",
  "title": "{{ cookiecutter.scraper_name }}",
  "version": "0.0",
  {% if cookiecutter.use_proxy -%}
  "input": {
    "title": "Input",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
      "proxyConfig": {
        "title": "Proxy config",
        "description": "Configuration for Apify Proxy",
        "type": "object",
        "editor": "proxy",
        "prefill": {
          "useApifyProxy": true,
          "apifyProxyGroups": ["RESIDENTIAL"]
        },
        "default": {
          "useApifyProxy": true,
          "apifyProxyGroups": ["RESIDENTIAL"]
        }
      }
    }
  },
  {% endif -%}
  "dockerfile": "../../../../../Dockerfile",
  "dockerContextDir": "../../../../../",
  "storages": {
    "dataset": "../../schemas/{{ cookiecutter.schema_name }}"
  }
}
